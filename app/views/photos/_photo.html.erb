<div class="photo-elem"><%- if photo.image_processed && photo.image_file.thumb_square && photo.image_file.single %><%= link_to image_tag( "", :class => "lazy", "data-original" => photo.image_file.thumb.url, "data-aspect-ratio" => photo.aspect_ratio), [photo.album,photo], "data-mfp-src" => photo.image_file.single.url, :title => photo.title, :class => "view", "data-capture-time" => ( photo.capture_time.blank? ? '' : photo.capture_time.strftime("%b %d %Y, %H:%M")  ), "data-user" => (photo.user ? photo.user.name : ""), "data-fullsize" => photo.image_file.file.url %><%- else %><%= link_to image_tag("processing.jpg", :class => "img-responsive "), @album %><%- end %></div>